table abs
vardef
  sp,Real,input;
  br,Real,input;
  ssa,Real,input;
  ssb,Real,input;
  abs,Real,output;
endvardef
reqdef
  ((sp>15)&(br>8))&((ssa==1)&(ssb==1)),abs==1;
endreqdef
endtable
